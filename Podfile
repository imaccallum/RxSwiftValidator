source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '10.0'
inhibit_all_warnings!
use_frameworks!


target 'RxSwiftValidator' do

	# Tools
	pod 'RxSwift',    '~> 3.0'
	pod 'SwiftValidators'
	pod 'SwiftValidator', :git => 'https://github.com/jpotts18/SwiftValidator.git', :branch => 'master'

end

target 'RxSwiftValidatorTests' do
	pod 'RxBlocking', '~> 3.0'
	pod 'RxTest',     '~> 3.0'
end